<form id="formCadastroBeneficiario" method="post">
    <div class="row align-bottom">
        <div class="col-md-5">
            <div class="form-group">
                <label for="CPFBeneficiario">CPF:</label>
                <input required="required" type="text" class="form-control" id="CPFBeneficiario" name="CPFBeneficiario" placeholder="Ex.: 999.999.999-99" maxlength="255">
            </div>
        </div>
        <div class="col-md-5">
            <div class="form-group">
                <label for="NomeBeneficiario">Nome:</label>
                <input required="required" type="text" class="form-control" id="NomeBeneficiario" name="NomeBeneficiario" placeholder="Ex.: João" maxlength="50">
            </div>
        </div>
        <div class="col-lg-2">
            <div class="form-group">
                <button type="button" class="btn btn-sm btn-success">Incluir</button>
            </div>
        </div>
    </div>
</form>