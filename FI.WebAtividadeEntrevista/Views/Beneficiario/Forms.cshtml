@model WebAtividadeEntrevista.Models.BeneficiarioModel
<form id="formCadastroBeneficiario" method="post">
    <div class="row align-bottom">
        <div class="col-md-5">
            <div class="form-group">
                @Html.LabelFor(m => m.CPF)
                @Html.TextBoxFor(m => m.CPF, new { placeholder = "Ex.: 999.999.999-99", @class = "form-control", id = "CPFBeneficiario", name = "CPFBeneficiario" })
            </div>
        </div>
        <div class="col-md-5">
            <div class="form-group">
                @Html.LabelFor(m => m.Nome)
                @Html.TextBoxFor(m => m.Nome, new { placeholder = "Ex.: João", @class = "form-control", id = "NomeBeneficiario", name = "NomeBeneficiario" })
            </div>
        </div>
        <div class="col-lg-2">
            <div class="form-group">
                <button type="button" class="btn btn-sm btn-success" id="btn-incluir-beneficiario">Incluir</button>
            </div>
        </div>
    </div>
</form>